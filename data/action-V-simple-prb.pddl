
(define (problem action-prb-simple)
    (:domain action)
    (:objects r2 r1 - robot
       obs_a2 obs_b2 obs_d2 obs_a1 obs_b1 obs_c1 obs_d1 - loc-obs
       wp_a2 wp_b2 wp_d2 wp_a1 wp_b1 wp_c1 wp_d1 wp_com - loc-wp)
    (:init (at-r r1 wp_a1)
      (at-r r2 wp_a2)
      (robot-allowed r2 wp_a2)
      (robot-allowed r2 wp_b2)
      (robot-allowed r2 wp_d2)
      (robot-allowed r2 wp_com)
      (robot-allowed r1 wp_a1)
      (robot-allowed r1 wp_b1)
      (robot-allowed r1 wp_c1)
      (robot-allowed r1 wp_d1)
      (robot-allowed r1 wp_com)
      (= (distance wp_a2 wp_b2) 1) (adjacent wp_a2 wp_b2)
      (= (distance wp_b2 wp_a2) 1) (adjacent wp_b2 wp_a2)
      (= (distance wp_b2 wp_com) 1) (adjacent wp_b2 wp_com)
      (= (distance wp_com wp_b2) 1) (adjacent wp_com wp_b2)
      (= (distance wp_com wp_d2) 1) (adjacent wp_com wp_d2)
      (= (distance wp_d2 wp_com) 1) (adjacent wp_d2 wp_com)
      (= (distance wp_a1 wp_b1) 1) (adjacent wp_a1 wp_b1)
      (= (distance wp_b1 wp_a1) 1) (adjacent wp_b1 wp_a1)
      (= (distance wp_b1 wp_c1) 1) (adjacent wp_b1 wp_c1)
      (= (distance wp_c1 wp_b1) 1) (adjacent wp_c1 wp_b1)
      (= (distance wp_a1 wp_c1) 1) (adjacent wp_a1 wp_c1)
      (= (distance wp_c1 wp_a1) 1) (adjacent wp_c1 wp_a1)
      (= (distance wp_a1 wp_com) 1) (adjacent wp_a1 wp_com)
      (= (distance wp_com wp_a1) 1) (adjacent wp_com wp_a1)
      (= (distance wp_com wp_d1) 1) (adjacent wp_com wp_d1)
      (= (distance wp_d1 wp_com) 1) (adjacent wp_d1 wp_com)
      (visible r2 wp_a2 obs_a2)
      (visible r2 wp_b2 obs_b2)
      (visible r2 wp_d2 obs_d2)
      (visible r1 wp_a1 obs_a1)
      (visible r1 wp_b1 obs_b1)
      (visible r1 wp_c1 obs_c1)
      (visible r1 wp_d1 obs_d1)
      (visible-com r1 r2 wp_com wp_com))
    (:goal (and (explored obs_a2)
      (explored obs_b2)
      (explored obs_d2)
      (explored obs_a1)
      (explored obs_b1)
      (explored obs_c1)
      (explored obs_d1)))
)
